{
  "name": "walnut-admin",
  "version": "0.0.1",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build:staging": "vite build --mode staging",
    "build:prod": "vite build",
    "update:easy": "npm i easy-fns-ts@latest",
    "lint": "eslint --ext .js,.vue src -f table",
    "lint:fix": "eslint --ext .js,.vue src -f table --fix",
    "lint:prettier": "prettier --write .",
    "lint:style": "stylelint src/assets/style/**/*.scss --fix"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "crypto-js": "^4.0.0",
    "easy-fns-ts": "^1.0.15",
    "element-plus": "^1.0.2-beta.28",
    "lodash-es": "^4.17.20",
    "screenfull": "^5.1.0",
    "sortablejs": "^1.10.2",
    "vue": "^3.0.5",
    "vue-i18n": "^9.0.0-rc.1",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0-rc.2"
  },
  "devDependencies": {
    "@iconify-icons/ant-design": "^1.1.0",
    "@iconify-icons/ic": "^1.1.1",
    "@iconify/vue": "^2.0.0-rc.2",
    "@vitejs/plugin-vue": "^1.0.5",
    "@vue/compiler-sfc": "^3.0.5",
    "babel-plugin-component": "^1.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-vue": "^7.3.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "sass": "^1.29.0",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "vite": "^2.0.0-beta.35"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run lint:fix",
      "npm run lint:prettier"
    ],
    "*.{scss}": [
      "npm run lint:style"
    ]
  }
}
