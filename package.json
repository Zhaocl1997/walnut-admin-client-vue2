{
  "name": "vite-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "vite build",
    "update:easy": "npm i easy-fns-ts@latest",
    "lint": "eslint --ext .js,.vue src -f table",
    "lint:fix": "eslint --ext .js,.vue src -f table --fix",
    "lint:prettier": "prettier --write ."
  },
  "dependencies": {
    "easy-fns-ts": "^1.0.10",
    "element-plus": "^1.0.1-beta.17",
    "screenfull": "^5.1.0",
    "sortablejs": "^1.10.2",
    "vue": "^3.0.4",
    "vue-i18n": "^9.0.0-beta.17",
    "vue-router": "^4.0.1",
    "vuex": "^4.0.0-rc.2"
  },
  "devDependencies": {
    "@iconify-icons/ant-design": "^1.1.0",
    "@iconify-icons/ic": "^1.1.1",
    "@iconify/vue": "^2.0.0-rc.2",
    "@vue/compiler-sfc": "^3.0.4",
    "babel-plugin-component": "^1.1.1",
    "cross-env": "^7.0.3",
    "dot-prop": "^6.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-vue": "^7.3.0",
    "glob": "^7.1.6",
    "globrex": "^0.1.2",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "sass": "^1.29.0",
    "vite": "^1.0.0-rc.13"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run lint",
      "npm run lint:prettier"
    ]
  }
}
